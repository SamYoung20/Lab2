#! /usr/local/bin/vvp
:ivl_version "10.1 (stable)" "(v10_1-107-gab6ae79)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x217d390 .scope module, "pwm_tb" "pwm_tb" 2 2;
 .timescale 0 0;
v0x21b5c60_0 .var "clk", 0 0;
v0x21b5d50_0 .var "compare", 7 0;
v0x21b5e20_0 .net "pwm", 0 0, L_0x21b5ff0;  1 drivers
v0x21b5f20_0 .var "rst", 0 0;
E_0x21a30a0 .event negedge, v0x21b5b20_0;
S_0x21a27a0 .scope module, "DUT" "pwm" 2 8, 3 1 0, S_0x217d390;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk"
    .port_info 1 /INPUT 1 "rst"
    .port_info 2 /INPUT 8 "compare"
    .port_info 3 /OUTPUT 1 "pwm"
P_0x21a2970 .param/l "CTR_LEN" 0 3 1, +C4<00000000000000000000000000001000>;
L_0x21b5ff0 .functor BUFZ 1, v0x21b5a60_0, C4<0>, C4<0>, C4<0>;
v0x217f7f0_0 .net "clk", 0 0, v0x21b5c60_0;  1 drivers
v0x21b55e0_0 .net "compare", 7 0, v0x21b5d50_0;  1 drivers
v0x21b56c0_0 .var "ctr_d", 7 0;
v0x21b57b0_0 .var "ctr_q", 7 0;
v0x21b5890_0 .net "pwm", 0 0, L_0x21b5ff0;  alias, 1 drivers
v0x21b59a0_0 .var "pwm_d", 0 0;
v0x21b5a60_0 .var "pwm_q", 0 0;
v0x21b5b20_0 .net "rst", 0 0, v0x21b5f20_0;  1 drivers
E_0x21a2e20 .event posedge, v0x217f7f0_0;
E_0x216b220 .event edge, v0x21b57b0_0, v0x21b55e0_0;
    .scope S_0x21a27a0;
T_0 ;
    %wait E_0x216b220;
    %load/vec4 v0x21b57b0_0;
    %addi 1, 0, 8;
    %store/vec4 v0x21b56c0_0, 0, 8;
    %load/vec4 v0x21b57b0_0;
    %load/vec4 v0x21b55e0_0;
    %cmp/u;
    %jmp/0xz  T_0.0, 5;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x21b59a0_0, 0, 1;
    %jmp T_0.1;
T_0.0 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x21b59a0_0, 0, 1;
T_0.1 ;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x21a27a0;
T_1 ;
    %wait E_0x21a2e20;
    %load/vec4 v0x21b5b20_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.0, 8;
    %pushi/vec4 0, 0, 8;
    %assign/vec4 v0x21b57b0_0, 0;
    %jmp T_1.1;
T_1.0 ;
    %load/vec4 v0x21b56c0_0;
    %assign/vec4 v0x21b57b0_0, 0;
T_1.1 ;
    %load/vec4 v0x21b59a0_0;
    %assign/vec4 v0x21b5a60_0, 0;
    %jmp T_1;
    .thread T_1;
    .scope S_0x217d390;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x21b5c60_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x21b5f20_0, 0, 1;
    %pushi/vec4 4, 0, 32;
T_2.0 %dup/vec4;
    %pushi/vec4 0, 0, 32;
    %cmp/s;
    %jmp/1xz T_2.1, 5;
    %jmp/1 T_2.1, 4;
    %pushi/vec4 1, 0, 32;
    %sub;
    %delay 10, 0;
    %load/vec4 v0x21b5c60_0;
    %inv;
    %store/vec4 v0x21b5c60_0, 0, 1;
    %jmp T_2.0;
T_2.1 ;
    %pop/vec4 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x21b5f20_0, 0, 1;
T_2.2 ;
    %delay 10, 0;
    %load/vec4 v0x21b5c60_0;
    %inv;
    %store/vec4 v0x21b5c60_0, 0, 1;
    %jmp T_2.2;
    %end;
    .thread T_2;
    .scope S_0x217d390;
T_3 ;
    %vpi_call 2 24 "$dumpfile", "pwm.vcd" {0 0 0};
    %vpi_call 2 25 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x21a27a0 {0 0 0};
    %pushi/vec4 0, 0, 8;
    %store/vec4 v0x21b5d50_0, 0, 8;
    %wait E_0x21a30a0;
    %pushi/vec4 128, 0, 8;
    %store/vec4 v0x21b5d50_0, 0, 8;
    %pushi/vec4 256, 0, 32;
T_3.0 %dup/vec4;
    %pushi/vec4 0, 0, 32;
    %cmp/s;
    %jmp/1xz T_3.1, 5;
    %jmp/1 T_3.1, 4;
    %pushi/vec4 1, 0, 32;
    %sub;
    %wait E_0x21a2e20;
    %jmp T_3.0;
T_3.1 ;
    %pop/vec4 1;
    %pushi/vec4 30, 0, 8;
    %store/vec4 v0x21b5d50_0, 0, 8;
    %pushi/vec4 256, 0, 32;
T_3.2 %dup/vec4;
    %pushi/vec4 0, 0, 32;
    %cmp/s;
    %jmp/1xz T_3.3, 5;
    %jmp/1 T_3.3, 4;
    %pushi/vec4 1, 0, 32;
    %sub;
    %wait E_0x21a2e20;
    %jmp T_3.2;
T_3.3 ;
    %pop/vec4 1;
    %vpi_call 2 32 "$finish" {0 0 0};
    %end;
    .thread T_3;
# The file index is used to find the file name in the following table.
:file_names 4;
